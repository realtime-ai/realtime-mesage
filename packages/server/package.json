{
  "name": "@realtime-mesage/server",
  "version": "1.0.0",
  "description": "Modular realtime message infrastructure built on Socket.IO and Redis",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/realtime-ai/realtime-mesage.git",
    "directory": "packages/server"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "ts-node src/server.ts",
    "start": "node dist/server.js",
    "test": "vitest run src",
    "test:watch": "vitest src",
    "test:e2e": "REDIS_RUNNING=1 vitest run src/e2e",
    "benchmark:presence": "node benchmark/presence-load-test.mjs",
    "benchmark:sdk": "node benchmark/sdk-presence-load-test.mjs",
    "benchmark:validate": "node benchmark/benchmark-validator.mjs",
    "benchmark:ci": "ROOM_COUNT=10 USERS_PER_ROOM=5 TEST_DURATION_MS=30000 node benchmark/benchmark-validator.mjs benchmark/presence-load-test.mjs",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "socket.io",
    "redis",
    "presence",
    "realtime"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@socket.io/redis-adapter": "^8.3.0",
    "dotenv": "^17.2.2",
    "ioredis": "^5.7.0",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@realtime-mesage/sdk": "workspace:*",
    "@types/ioredis-mock": "^8.2.6",
    "ioredis-mock": "^8.9.0",
    "socket.io-client": "^4.8.1",
    "ts-node": "^10.9.2"
  }
}

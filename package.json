{
  "name": "realtime-mesage",
  "version": "1.0.0",
  "private": true,
  "description": "Realtime presence service built with Socket.IO and Redis",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/realtime-ai/realtime-message.git"
  },
  "workspaces": [
    "backend",
    "realtime-message-sdk"
  ],
  "scripts": {
    "build": "npm run build --workspace backend",
    "build:sdk": "npm run build --workspace realtime-message-sdk",
    "dev": "npm run dev --workspace backend",
    "start": "npm run start --workspace backend",
    "test": "npm run test --workspace backend && npm run test --workspace realtime-message-sdk",
    "test:server": "npm run test --workspace backend",
    "test:sdk": "npm run test --workspace realtime-message-sdk",
    "test:e2e": "npm run test:e2e --workspace backend",
    "test:all": "npm run test --workspace backend && npm run test --workspace realtime-message-sdk && npm run test:e2e --workspace backend",
    "test:watch": "npm run test:watch --workspace backend",
    "prepare": "husky",
    "benchmark:presence": "npm run benchmark:presence --workspace backend",
    "benchmark:sdk": "npm run benchmark:sdk --workspace backend",
    "benchmark:validate": "npm run benchmark:validate --workspace backend",
    "benchmark:ci": "npm run benchmark:ci --workspace backend",
    "sdk:demo": "npm run demo --workspace realtime-message-sdk"
  },
  "keywords": [
    "socket.io",
    "redis",
    "presence"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "husky": "^9.1.7"
  }
}
